#:kivy 1.0.9

<GUI>:
    canvas:
        Color:
            rgb: (255, 255, 255)
        Rectangle:
            pos: self.pos
            pos: self.size
    Label:
        font_size: 40
        center_x: root.width / 5
        top: root.top - 50
        text: "New Order"

    Label:
        font_size: 40
        center_x: root.width / 5
        top: root.top - 200
        text: "Cancel Order"

